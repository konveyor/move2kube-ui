{
    "name": "move2kube",
    "version": "0.1.0",
    "description": "An UI for Move2Kube.",
    "license": "Apache-2.0",
    "repository": "https://github.com/konveyor/move2kube-ui.git",
    "packageManager": "pnpm@7.8.0",
    "engines": {
        "node": "16.16.0"
    },
    "scripts": {
        "start": "move2kube-api --log-level=trace --static-files-dir=dist",
        "start:image": "docker run --rm -it -p 8080:8080 quay.io/konveyor/move2kube-ui move2kube-api --port 8080 --log-level trace --static-files-dir dist",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --fix --max-warnings 0",
        "build": "webpack --config webpack.prod.js && echo 'https://surge.sh/help/adding-a-200-page-for-client-side-routing' && cp dist/index.html dist/200.html"
    },
    "dependencies": {
        "@patternfly/patternfly": "^4.202.1",
        "@patternfly/react-core": "^4.224.1",
        "@patternfly/react-icons": "^4.75.1",
        "@patternfly/react-table": "^4.93.1",
        "js-yaml": "^4.1.0",
        "prop-types": "15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-flow-renderer": "^10.3.12",
        "react-router-dom": "^5.3.3",
        "react-router-last-location": "^2.0.1",
        "tslib": "^2.4.0"
    },
    "devDependencies": {
        "@types/js-yaml": "^4.0.5",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.0.0",
        "dotenv-webpack": "^8.0.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.6.1",
        "prettier": "^2.7.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.3.1",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "typescript": "^4.7.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.9.3",
        "webpack-merge": "^5.8.0"
    }
